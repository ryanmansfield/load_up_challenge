<div id="<%= dom_id booking %>" class="container">
  <p>
    <strong>First name:</strong>
    <%= booking.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= booking.last_name %>
  </p>

  <p>
    <strong>Animal name:</strong>
    <%= booking.animal_name %>
  </p>

  <p>
    <strong>Animal type:</strong>
    <%= booking.animal_type %>
  </p>

  <p>
    <strong>Hours:</strong>
    <%= booking.hours %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= booking.date %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= booking.price %>
  </p>

</div>
